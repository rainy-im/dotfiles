{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "commandPalette"
            },
            "keys": "ctrl+shift+p"
        },
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "vertical"
            },
            "keys": "alt+ctrl+plus"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "horizontal"
            },
            "keys": "alt+ctrl+-"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+ctrl+d"
        },
        {
            "command": "closePane",
            "keys": "ctrl+d"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
    "language": "zh-Hans-CN",
